import java.util.Scanner;

public class Comparison {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner input = new Scanner (System.in);
		int a= input.nextInt();
		int b= input.nextInt();
		int c= input.nextInt();
		int max=0;
		if (a > b) {max = a;
		      {if (c>a) max = c;}}
		System.out.println(max);
	}

}

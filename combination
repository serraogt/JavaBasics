public class combination {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int possibilityCounter = 0;
		
		for (int i= 1;i <=4; i++) {
			for (int j = 1; j <=4; j++) {
				for (int k = 1; k<=4; k++  ) {
					
					if (i != k && i != j && k != j) {
						System.out.print(i);
						System.out.print(j);
						System.out.println(k);
						possibilityCounter++;
					}
				}
			}
		}
		System.out.println(possibilityCounter);}
 
